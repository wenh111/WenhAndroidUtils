    <declare-styleable name="LoadingProgressBarView">
        <attr name="android:text" />
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="ffProgress" format="integer" />
        <attr name="ffMax" format="integer" />
        <attr name="ffStartAngle" format="integer" />
        <attr name="ffInverted" format="boolean" />
        <attr name="ffCounterclockwise" format="boolean" />
        <attr name="ffStrokeWidth" format="dimension" />
        <attr name="ffBackgroundColor" format="reference|color" />
        <attr name="ffProgressColor" format="reference|color" />
        <attr name="ffStrokeColor" format="reference|color" />
        <attr name="ffShowStroke" format="boolean" />
        <attr name="ffShowText" format="boolean" />
        <attr name="ffTypeface" format="string" />
        <attr name="ffImage" format="reference" />
        <attr name="ffProgressFillType" format="enum">
            <enum name="radial" value="0" />
            <enum name="center" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ShadowRelativeLayout">
        <attr name="shadow_color" format="color" />
