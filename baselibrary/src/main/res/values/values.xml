    <declare-styleable name="LoadingProgressBarView">
        <attr name="android:text"/>
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr format="integer" name="ffProgress"/>
        <attr format="integer" name="ffMax"/>
        <attr format="integer" name="ffStartAngle"/>
        <attr format="boolean" name="ffInverted"/>
        <attr format="boolean" name="ffCounterclockwise"/>
        <attr format="dimension" name="ffStrokeWidth"/>
        <attr format="reference|color" name="ffBackgroundColor"/>
        <attr format="reference|color" name="ffProgressColor"/>
        <attr format="reference|color" name="ffStrokeColor"/>
        <attr format="boolean" name="ffShowStroke"/>
        <attr format="boolean" name="ffShowText"/>
        <attr format="string" name="ffTypeface"/>
        <attr format="reference" name="ffImage"/>
        <attr format="enum" name="ffProgressFillType">
            <enum name="radial" value="0"/>
            <enum name="center" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ShadowRelativeLayout">
        <attr format="color" name="shadow_color"/>
        <attr format="dimension" name="shadow_radius"/>
        <attr format="dimension" name="shadow_blur"/>
        <attr format="dimension" name="shadow_dx"/>
        <attr format="dimension" name="shadow_dy"/>
    </declare-styleable>
    <declare-styleable name="SwitchButton" ns1:ignore="ResourceName">
        <attr format="reference" name="switchThumbDrawable"/>
        <attr format="color|reference" name="switchThumbColor"/>
        <attr format="dimension|reference" name="switchThumbMargin"/>
        <attr format="dimension|reference" name="switchThumbMarginTop"/>
        <attr format="dimension|reference" name="switchThumbMarginBottom"/>
        <attr format="dimension|reference" name="switchThumbMarginLeft"/>
        <attr format="dimension|reference" name="switchThumbMarginRight"/>
        <attr format="dimension|reference" name="switchThumbWidth"/>
        <attr format="dimension|reference" name="switchThumbHeight"/>
        <attr format="dimension|reference" name="switchThumbRadius"/>
        <attr format="dimension|reference" name="switchBackRadius"/>
        <attr format="reference" name="switchBackDrawable"/>
        <attr format="color|reference" name="switchBackColor"/>
        <attr format="boolean" name="switchFadeBack"/>
        <attr format="float" name="switchThumbRangeRatio"/>
        <attr format="integer" name="switchAnimationDuration"/>
        <attr format="color|reference" name="switchTintColor"/>
        <attr format="string" name="switchTextOn"/>
        <attr format="string" name="switchTextOff"/>
        <attr format="dimension" name="switchTextThumbInset"/>
        <attr format="dimension" name="switchTextExtra"/>
        <attr format="dimension" name="switchTextAdjust"/>
    </declare-styleable>
    <declare-styleable name="UpdateAppNumberProgressBar">
        <attr format="integer" name="progress_current"/>
        <attr format="integer" name="progress_max"/>

        <attr format="color" name="progress_unreached_color"/>
        <attr format="color" name="progress_reached_color"/>

        <attr format="dimension" name="progress_reached_bar_height"/>
        <attr format="dimension" name="progress_unreached_bar_height"/>

        <attr format="dimension" name="progress_text_size"/>
        <attr format="color" name="progress_text_color"/>

        <attr format="dimension" name="progress_text_offset"/>

        <attr format="enum" name="progress_text_visibility">
            <enum name="visible" value="0"/>
            <enum name="invisible" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="Widget">
        <attr name="android:text"/>
        <attr name="android:color"/>
        <attr name="android:maxHeight"/>
    </declare-styleable>
