        <attr name="switchThumbDrawable" format="reference" />
        <attr name="switchThumbColor" format="color|reference" />
        <attr name="switchThumbMargin" format="dimension|reference" />
        <attr name="switchThumbMarginTop" format="dimension|reference" />
        <attr name="switchThumbMarginBottom" format="dimension|reference" />
        <attr name="switchThumbMarginLeft" format="dimension|reference" />
        <attr name="switchThumbMarginRight" format="dimension|reference" />
        <attr name="switchThumbWidth" format="dimension|reference" />
        <attr name="switchThumbHeight" format="dimension|reference" />
        <attr name="switchThumbRadius" format="dimension|reference" />
        <attr name="switchBackRadius" format="dimension|reference" />
        <attr name="switchBackDrawable" format="reference" />
        <attr name="switchBackColor" format="color|reference" />
        <attr name="switchFadeBack" format="boolean" />
        <attr name="switchThumbRangeRatio" format="float" />
        <attr name="switchAnimationDuration" format="integer" />
        <attr name="switchTintColor" format="color|reference" />
